#!/usr/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'vel-err==0.0.0','console_scripts','reg = vel_err.drone_rek:mainclient'
__requires__ = 'vel-err==0.0.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('vel-err==0.0.0', 'console_scripts', 'reg = vel_err.drone_rek:mainclient')()
    )
